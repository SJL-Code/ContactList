<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="waveSideBar">

        <!-- *,A,B,C... 这些字母的大小 -->
        <attr name="textSize" format="dimension" />

        <!-- #,A,B,C... 这些字母的颜色 -->
        <attr name="textColor" format="color|reference" />

        <!-- #,A,B,C... 这些字母被选中时的颜色 -->
        <attr name="chooseTextColor" format="color|reference" />

        <!-- 点击*,A,B,C... 这些字母时出现提示字母的大小 -->
        <attr name="hintTextSize" format="dimension" />

        <!-- 点击*,A,B,C... 这些字母时出现提示圆的颜色 -->
        <attr name="backgroundColor" format="color|reference" />

        <!-- 点击*,A,B,C... 这些字母时出现提示圆的半径 -->
        <attr name="circleRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ClearEditTextStyle">
        <attr name="leftImage" format="reference" />
        <attr name="rightImage" format="reference" />
    </declare-styleable>
</resources>